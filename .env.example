# Frontend Environment Variables
# Copy this file to .env and update with your values

# Port for the React development server
PORT=3002

# Main API (monitoring service)
# For local development: http://localhost:3014
# For remote deployment: http://YOUR_SERVER_IP:3014
REACT_APP_API_URL=http://192.168.1.64:3014

# Individual microservice endpoints
# Update YOUR_SERVER_IP with your actual server IP address
REACT_APP_WEBHOOK_GATEWAY_URL=http://YOUR_SERVER_IP:3010
REACT_APP_TRADE_ORCHESTRATOR_URL=http://YOUR_SERVER_IP:3013
REACT_APP_TRADOVATE_SERVICE_URL=http://YOUR_SERVER_IP:3011
REACT_APP_MARKET_DATA_SERVICE_URL=http://YOUR_SERVER_IP:3012

# React build settings
GENERATE_SOURCEMAP=false